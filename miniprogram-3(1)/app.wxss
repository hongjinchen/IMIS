/**app.wxss**/
@import "./styles/iconfont.wxss";
page, view, text, swiper, swiper-item, image, navigator{
  background: #F3F3F3;
  font-size: 23rpx;
  padding: 0;
  margin: 0;
  box-sizing: border-box;

}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}  